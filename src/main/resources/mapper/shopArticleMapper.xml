<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.bit.backpackers.shop.model.ShopArticleDao">
  	<select id="selectAll" resultType="com.bit.backpackers.shop.model.entity.ShopArticleVo">
  		SELECT * FROM shop_article;
  	</select>
  	<select id="selectOne" parameterType="String" resultType="com.bit.backpackers.shop.model.entity.ShopArticleVo">
  		SELECT * FROM shop_article WHERE shopArticleCode = #{shopArticleCode}
  	</select>
  </mapper>